# Overall env.
APM_ENV="dev"

# Basic Node inironment variables.
NODE_ENV="production"
NODE_ARGS="--debug"
INSTANCES=0

# React Perf variable.
USE_PERF=false

# Source map and logging level.
LOGGING_LEVEL="INFO"

# What workspace to watch ("server", "client", "all").
WATCH_CLIENT=false
WATCH_SERVER=false

# ELB Config
CLIENT_DOMAIN_NAME="dev.obo"
CLIENT_PORT=443
CLIENT_HTTPS=true

# Authentication service:
AUTH_SERVICE_BASE="http://internal.dev.obo/auth/api/v0"

# enum service:
ENUM_SERVICE_BASE="http://internal.dev.obo/sor/api/v0"

# System of record service:
SOR_SERVICE_BASE="http://internal.dev.obo/sor/api/v0"

# Interview service:
INTV_SERVICE_BASE="http://internal.dev.obo/auth/api/v0"

# Survey service:
SURVEY_SERVICE_BASE="http://internal.dev.obo/survey/api/v0"

# SurveyHandler service:
SH_SERVICE_BASE="http://internal.dev.obo/survey/api/v0"

# Import service:
IMPORT_SERVICE_BASE="http://internal.dev.obo/import/api/v0"

# Profile service:
PROFILE_SERVICE_BASE="http://internal.dev.obo/auth/api/v0"

STORYBOOK_ENABLED=true

# UI Runtime vars ==================================================|

# Logging & devtools.
UI_ENABLE_DEVTOOLS=true
UI_LOGGING_LEVEL="debug"

