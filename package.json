{
  "name": "web-app",
  "version": "1.30.0-SNAPSHOT",
  "author": "agile-pm",
  "repository": "https://github.com/agile-pm/web-app.git",
  "main": "./dist/server/index.js",
  "engines": {
    "node": ">=6.0.0 <7.0.0"
  },
  "bin": "./dist/server/index.js",
  "pkg": {
    "targets": [
      "node6",
      "macos"
    ],
    "scripts": [
      "./dist/server/**/*.js"
    ]
  },
  "scripts": {
    "watch": "nodemon --exec npm run watch:serve | bunyan -o short",
    "watch:serve": "nodemon --inspect init.env.js  --exec babel-node --presets es2015,stage-2",
    "build": "del-cli dist && npm run lint && babel src -d dist",
    "start": "node src/server/index.js | bunyan -o short",
    "build:development": "NODE_ENV=development WATCH_CLIENT=false WATCH_SERVER=false gulp build",
    "build:production": "NODE_ENV=production WATCH_CLIENT=false WATCH_SERVER=false gulp build",
    "setup:staging": "gulp setup -e staging",
    "setup:production": "gulp setup -e production",
    "docker:build": "cd ../ && docker build -f web-app/Dockerfile -t web-app:staging .",
    "docker:run": "docker run -i -t -p 3333:3333 --name=\"web-app\" web-app:staging",
    "docker:rm": "docker rm web-app",
    "docker:stop": "docker stop local/web-app",
    "docker:kill": "docker kill local/web-app",
    "compile-bin": "pkg --out-path ./dist/bin ."
  },
  "dependencies": {
    "ajv": "^5.1.0",
    "ajv-errors": "^1.0.0",
    "ajv-i18n": "^2.0.2",
    "ajv-keywords": "^2.1.0",
    "async": "^1.5.2",
    "aws-sdk": "^2.6.11",
    "axios": "^0.15.3",
    "b64": "^3.0.2",
    "bassmaster": "^2.0.2",
    "boom": "^4.2.0",
    "bunyan": "^1.8.12",
    "co": "https://registry.npmjs.org/co/-/co-4.6.0.tgz",
    "cookie_js": "^1.2.2",
    "cq-prolyfill": "^0.4.0",
    "crumb": "^6.0.3",
    "fast-safe-stringify": "https://registry.npmjs.org/fast-safe-stringify/-/fast-safe-stringify-1.1.13.tgz",
    "file-system": "^2.2.2",
    "flexibility": "^2.0.1",
    "fs-extra": "^5.0.0",
    "fuse.js": "^2.6.1",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.0.1",
    "hapi-auth-cookie": "^7.0.0",
    "hapi-test": "^5.1.0",
    "hapijs-status-monitor": "^0.5.0",
    "history": "https://registry.npmjs.org/history/-/history-3.3.0.tgz",
    "hoek": "https://registry.npmjs.org/hoek/-/hoek-4.1.0.tgz",
    "inert": "^4.0.3",
    "iron": "https://registry.npmjs.org/iron/-/iron-4.0.5.tgz",
    "jStat": "^1.5.3",
    "joi": "^10.0.6",
    "js-beautify": "^1.6.14",
    "json-logic-js": "^1.2.1",
    "jwt-decode": "^2.2.0",
    "lodash.debounce": "https://registry.npmjs.org/lodash.debounce/-/lodash.debounce-3.1.1.tgz",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.17.1",
    "moment-timezone": "https://registry.npmjs.org/moment-timezone/-/moment-timezone-0.5.13.tgz",
    "morgan": "^1.8.2",
    "platform": "^1.3.4",
    "qs": "^6.3.0",
    "rimraf": "^2.6.2",
    "uuid": "^3.0.0",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "columnify": "^1.5.4",
    "dotenv": "^2.0.0",
    "expect": "^1.20.2",
    "file-loader": "^0.8.5",
    "glob": "https://registry.npmjs.org/glob/-/glob-7.1.1.tgz",
    "happypack": "^3.0.3",
    "husky": "^0.14.3",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "nodemon": "^1.14.3",
    "remotedev-serialize": "^0.1.0",
    "yargs": "https://registry.npmjs.org/yargs/-/yargs-3.32.0.tgz"
  }
}
